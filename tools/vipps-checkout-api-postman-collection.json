        {
          "id": "871cbdfc-6a0b-41f4-8ac2-5dd0163113ec",
          "name": "Create a Checkout session",
          "request": {
            "name": "Create a Checkout session",
            "description": {
              "content": "Creates a checkout session and returns a JWT token which wraps the login URL,\r\necom landing page URL, free standing card page URL.",
              "type": "text/plain"
            },
            "url": {
              "path": [
                "v2",
                "session"
              ],
              "host": [
                "{{base_url}}/checkout"
              ],
              "query": [],
              "variable": []
            },
            "header": [
              {
                "disabled": false,
                "key": "Vipps-System-Name",
                "value": "{{vipps_system_name}}",
                "description": ""
              },
              {
                "disabled": false,
                "key": "Vipps-System-Version",
                "value": "{{vipps_system_version}}",
                "description": ""
              },
              {
                "disabled": false,
                "key": "Vipps-System-Plugin-Name",
                "value": "{{vipps_system_plugin_name}}",
                "description": ""
              },
              {
                "disabled": false,
                "key": "Vipps-System-Plugin-Version",
                "value": "{{vipps_system_plugin_version}}",
                "description": ""
              },
              {
                "disabled": false,
                "key": "client_id",
                "value": "{{client_id}}",
                "description": ""
              },
              {
                "disabled": false,
                "key": "client_secret",
                "value": "{{client_secret}}",
                "description": ""
              },
              {
                "disabled": false,
                "key": "Ocp-Apim-Subscription-Key",
                "value": "{{Ocp-Apim-Subscription-Key}}",
                "description": ""
              },
              {
                "disabled": false,
                "key": "Merchant-Serial-Number",
                "value": "{{merchantSerialNumber}}",
                "description": ""
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "method": "POST",
            "auth": null,
            "body": {
              "mode": "raw",
              "raw": "{\n  \"merchantInfo\": {\n    \"callbackPrefix\": \"{{callbackPrefix}}\",\n    \"returnUrl\": \"{{fallBack}}\",\n    \"callbackAuthorizationToken\": \"{{$guid}}\",\n    \"termsAndConditionsUrl\": \"{{termsAndConditionsUrl}}\"\n  },\n  \"transaction\": {\n    \"amount\": {\n      \"currency\": \"{{currency}}\",\n      \"value\": 1000\n    },\n    \"reference\": \"{{orderId}}\",\n    \"paymentDescription\": \"{{transactionTextInitiate}}\"\n  },\n  \"logistics\": {\n    \"dynamicOptionsCallback\": \"{{dynamicOptionsCallbackUrl}}\",\n    \"fixedOptions\": [\n      {\n        \"amount\": {\n          \"currency\": \"{{currency}}\",\n          \"value\": 300\n        },\n        \"id\": \"postenservicepakke1\",\n        \"priority\": 1,\n        \"brand\": \"Posten\",\n        \"product\": \"home delivery\",\n        \"isDefault\": true,\n        \"description\": \"Your package is delivered to your home.\",\n        \"isPickupPoint\": false,\n        \"isPorterbuddy\": false,\n        \"isInstabox\": false\n      },\n      {\n        \"amount\": {\n          \"currency\": \"{{currency}}\",\n          \"value\": 300\n        },\n        \"id\": \"postenservicepakke2\",\n        \"priority\": 2,\n        \"brand\": \"Posten\",\n        \"product\": \"pick-up in store\",\n        \"isDefault\": false,\n        \"description\": \"Pick up your package at the local store.\",\n        \"isPickupPoint\": true,\n        \"isPorterbuddy\": false,\n        \"isInstabox\": false\n      }\n    ],\n    \"integrations\": {\n      \"porterbuddy\": {\n        \"publicToken\": \"eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1Ni \",\n        \"apiKey\": \"eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1Ni \",\n        \"origin\": {\n          \"name\": \"{{firstName}} {{lastName}}\",\n          \"email\": \"{{email}}\",\n          \"phoneNumber\": \"{{internationalMobileNumber}}\",\n          \"address\": {\n            \"streetAddress\": \"{{streetAddress}}\",\n            \"postalCode\": \"{{postalCode}}\",\n            \"city\": \"{{city}}\",\n            \"country\": \"{{country}}\"\n          }\n        }\n      }\n    }\n  },\n  \"prefillCustomer\": {\n    \"firstName\": \"{{firstName}}\",\n    \"lastName\": \"{{lastName}}\",\n    \"email\": \"{{email}}\",\n    \"phoneNumber\": \"{{internationalMobileNumber}}\",\n    \"streetAddress\": \"{{streetAddress}}\",\n    \"city\": \"{{city}}\",\n    \"postalCode\": \"{{postalCode}}\",\n    \"country\": \"{{country}}\"\n  },\n  \"customerInteraction\": \"customer_not_present\",\n  \"contactFields\": true,\n  \"addressFields\": true\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if(pm.response.code === 200) { \r",
                  "    var jsonData = pm.response.json();\r",
                  "    var str = jsonData.pollingUrl\r",
                  "    var str2 = str.split('session/')[1]\r",
                  "    pm.environment.set('sessionId', str2);\r",
                  "}"
                ],
                "type": "text/javascript"
              }
            },
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "pm.environment.set('orderId',pm.environment.get('merchantSerialNumber') + (new Date()).getTime());"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "protocolProfileBehavior": {
            "disableBodyPruning": true
          }
        },
